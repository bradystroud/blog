{
  "name": "bradystroud",
  "version": "0.1.0",
  "packageManager": "bun@1.3.7",
  "scripts": {
    "postinstall": "bun scripts/patch-tinacms.js || true",
    "dev": "bun run --bun tinacms dev -c \"next dev\"",
    "build": "bun run --bun tinacms build && bun run --bun next build && bun run scripts/generate-rss.ts && bun run --bun next-sitemap --config next-sitemap.config.js",
    "generate-rss": "bun run scripts/generate-rss.ts",
    "start": "bun run --bun tinacms build && bun run --bun next start",
    "export": "bun run build && bun run --bun next export",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.1",
    "@svgr/webpack": "^8.1.0",
    "@tinacms/cli": "^2.0.7",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "25.0.10",
    "@types/react": "^19.2.8",
    "@types/styled-components": "^5.1.36",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "9.39.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "postcss-nesting": "^14.0.0"
  },
  "dependencies": {
    "@giscus/react": "^3.1.0",
    "@headlessui/react": "2.2.9",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/analytics": "^1.6.1",
    "@vercel/og": "^0.8.6",
    "@vercel/speed-insights": "^1.3.1",
    "gray-matter": "^4.0.3",
    "next": "16.1.4",
    "openai": "^6.16.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "tinacms": "^3.2.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "final-form": "4.20.1",
    "@types/node": "22.13.10"
  }
}
